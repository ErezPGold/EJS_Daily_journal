<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post"> 
    <div class="form-group">
        <label >Title</label>   
        <input name="postTitle" type="text" class="form-control" autocomplete="off" >
    </div>
    <div class="form-group">
        <label >Post</label>
        <textarea name="postBody" class="form-control" aria-label="With textarea" rows="5" autocomplete="off"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") -%>